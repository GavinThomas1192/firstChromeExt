{
    "manifest_version": 2,
    "name": "Bandwidth Hero",
    "description": "Saves data by compressing images on the page.",
    "version": "1.3.3",
    "content_scripts": [
        {
            "css": [
                "bundle.css"
            ],
            "matches": [
                "https://google.com/*"
            ]
        }
    ],
    "browser_action": {
        "default_icon": "assets/icon.png",
        "default_title": "Bandwidth Hero",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": [
            "background.bundle.js"
        ]
    },
    "permissions": [
        "activeTab",
        "tabs",
        "storage",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ]
}