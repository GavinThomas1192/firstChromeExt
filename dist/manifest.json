{
    "manifest_version": 2,
    "name": "QuickNote",
    "description": "Simple Note app",
    "version": "1.3.3",
    "browser_action": {
        "default_icon": "assets/icon.png",
        "default_title": "QuickNote",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": [
            "background.bundle.js"
        ]
    },
    "permissions": [
        "activeTab",
        "tabs",
        "storage",
        "webRequest",
        "webRequestBlocking",
        "contextMenus",
        "<all_urls>"
    ]
}